
<section class="container-fluid px-0 bctn-sign">
  <div class="w-100 py-3 ctn-loghead">
  <%= link_to home_index_path, class: 'mx-3' do %>
    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24">
    <path fill="currentColor" d="M20 11H7.83l5.59-5.59L12 4l-8 8l8 8l1.41-1.41L7.83 13H20v-2z"/>
    </svg>
    <%end%>
    <p class="my-0">LOGIN</p>

  </div>

  <div class="container ">
    <%= image_tag "city.png", class: 'img-city' %>
  </div>

  <h2 class="text-center py-3 fw-bold"> Welcome Back!</h2>

  <div class="container ctn-form ">
  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    
    <div class="field my-1 >
      <%= f.label :email_address  %>
      <%= f.email_field :email, autofocus: true, autocomplete: "email", class: " focus:ring-0"%>
    </div>
  
    <div class="field">
      <div class="pass d-flex justify-content-between">
      <%= f.label :password %>
      <%= link_to 'Forgot password?', new_user_password_path, class: ''  %>
      </div>
     
      <%= f.password_field :password, autocomplete: "current-password" %>
    </div>
  
  
   
  
  
    <div class="actions">
    <%= f.submit "Log in", class: " ac-btn btn btn-primary"  %>
  </div>
  <% end %>
  </div>
  
  
</section>



